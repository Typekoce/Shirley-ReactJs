var app = {};
var model = {};
var view = {};
var controller = {};
app.main = function(){
  alert("foo");
  app.menu = document.getElementById("menu");
  app.h1 = document.createElement("h1");
  app.h1.innerHTML = "Welcome to Shirley React Js";
  app.menu.appendChild(app.h1);
};
window.addEventListener('load',
  function() {
    alert('hello!');
    app.main();
  }, false);
